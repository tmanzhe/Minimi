{
    "name": "minimi",
    "description": "minimizing your contents!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions":[
        "activeTab",
        "storage",
        "sidePanel",
        "scripting"      
    ],
    "action":{
        "default_title": "minimi",
        "default_icon": "minimi_48x48.png"
    },
    "side_panel":{
        "default_path": "sidepanel.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "http://localhost:8080/*",
        "<all_urls>"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }

    
   
  }